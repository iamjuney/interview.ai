{
	"name": "interview.ai",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check .",
		"format": "prettier --write .",
		"db:generate": "drizzle-kit generate:pg --config=drizzle.config.ts",
		"db:push": "drizzle-kit push:pg --config=drizzle.config.ts",
		"db:up": "drizzle-kit up:pg",
		"db:studio": "drizzle-kit studio",
		"db:seed": "node --import tsx --env-file .env.local ./src/lib/db/seed.ts",
		"db:delete": "node --import tsx --env-file .env.local ./src/lib/db/delete.ts"
	},
	"devDependencies": {
		"@playwright/test": "^1.41.2",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/formidable": "^3.4.5",
		"@types/uuid": "^9.0.8",
		"autoprefixer": "^10.4.16",
		"dotenv": "^16.4.1",
		"dotenv-expand": "^10.0.0",
		"drizzle-kit": "^0.20.14",
		"esbuild-register": "^3.5.0",
		"postcss": "^8.4.32",
		"postcss-load-config": "^5.0.2",
		"prettier": "^3.2.4",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.5.11",
		"svelte": "^5.0.0-next.51",
		"svelte-check": "^3.6.0",
		"sveltekit-superforms": "^1.13.4",
		"tailwindcss": "^3.3.6",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3",
		"zod": "^3.22.4"
	},
	"type": "module",
	"dependencies": {
		"@ffmpeg/ffmpeg": "^0.12.6",
		"@ffmpeg/util": "^0.12.1",
		"@lucia-auth/adapter-postgresql": "^2.0.2",
		"@types/nprogress": "^0.2.3",
		"@vercel/blob": "^0.21.0",
		"@vercel/postgres": "^0.7.2",
		"ai": "^2.2.33",
		"bits-ui": "^0.16.0",
		"clsx": "^2.1.0",
		"drizzle-orm": "^0.29.3",
		"formidable": "^3.5.1",
		"install": "^0.13.0",
		"lucia": "^2.7.6",
		"lucide-svelte": "^0.321.0",
		"microsoft-cognitiveservices-speech-sdk": "^1.34.0",
		"mode-watcher": "^0.2.0",
		"nprogress": "^0.2.0",
		"openai": "^4.26.0",
		"oslo": "^1.0.4",
		"pg": "^8.11.3",
		"radix-icons-svelte": "^1.2.1",
		"slugify": "^1.6.6",
		"tailwind-merge": "^2.2.1",
		"tailwind-variants": "^0.1.20",
		"tsx": "^4.7.0",
		"uuid": "^9.0.1"
	}
}
